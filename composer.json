{
    "name": "nature-and-style/theme",
    "description": "Nature and Style Theme",
    "type": "oxideshop-theme",
    "keywords": [
        "oxid",
        "themes",
        "eShop"
    ],
    "homepage": "https://www.natureandstyle.com",
    "license": [
        "GPL-3.0-only"
    ],
    "minimum-stability": "dev",
    "prefer-stable": ture,
    "extra": {
        "oxideshop": {
            "target-directory": "nastheme",
            "assets-directory": "out/nastheme",
            "blacklist-filter": [
                "build/**/*",
                ".gitignore",
                ".gitattributes",
                "CHANGELOG.md",
                "composer.json",
                "CONTRIBUTING.md",
                "Gruntfile.js",
                "package.json",
                "package-lock.json"
            ]
        }
    }
}
